{
    "name": "parshikovpavel/array-cache",
    "description": "Non-persistent PSR-6/PSR-16 compatible cache using an array. Useful for mocking the original cache in tests",
    "keywords": ["cache", "array", "psr-6", "psr-16", "psr6", "psr16", "mock", "mocking"],
    "homepage": "https://github.com/parshikovpavel/array-cache",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Parshikov",
            "email": "paw-p@yandex.ru",
            "role": "Author"
        }
    ],
    "support": {
        "email": "paw-p@yandex.ru"
    },
    "readme": "https://github.com/parshikovpavel/array-cache/blob/master/README.md",
    "require": {
        "php": "^7.1",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0",
        "parshikovpavel/psr-cache-adapter" : "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ppCache\\": ["src/", "tests/"]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7"
    }
}
